package qa.ybl.pages;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.PageFactory;

import io.appium.java_client.pagefactory.AndroidFindBy;
import io.appium.java_client.pagefactory.AppiumFieldDecorator;

import qa.yblapp.base.Base;

public class HomePage extends Base {
	
	@AndroidFindBy(xpath = "//android.widget.TextView[@resource-id='com.atyati.ganaseva.mfi:id/android_gridview_text' and @text='Quick Data Entry']")
	private WebElement QDE;
	
	@AndroidFindBy(xpath = "")
	private WebElement CenterAndGroup;
	
	
	public HomePage() {
		super();
		PageFactory.initElements(new AppiumFieldDecorator(driver), this);
	}

}
